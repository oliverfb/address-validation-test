GOOGLE_MAPS_API_KEY=your-google-maps-platform-key_with-address-validation_and_places_enabled
PORT=3000

# Optional: when set, clients must send it via `X-API-Key`.
# If unset, the service will allow all requests (NOT recommended for public deployments).
X_API_KEY=your-internal-api-key_for_calling_this_service

# Optional: per-IP rate limiting (via @fastify/rate-limit).
RATE_LIMIT_ENABLED=true
RATE_LIMIT_MAX=60
RATE_LIMIT_WINDOW_MS=60000
